{
   "name": "@swc/css-core",
   "type": "module",
   "files": [
      "dist/",
      "src/"
   ],
   "exports": {
      "./package.json": "./package.json",
      ".": {
         "source": "./src/index.ts",
         "types": "./dist/index.ts",
         "import": "./src/index.ts",
         "require": "./src/index.ts"
      },
      "./Visitor": {
         "source": "./src/Visitor.ts",
         "types": "./dist/Visitor.ts",
         "import": "./src/Visitor.ts",
         "require": "./src/Visitor.ts"
      },
      "./*": {
         "source": "./src/*.ts",
         "types": "./dist/*.ts",
         "import": "./src/*.ts",
         "require": "./src/*.ts"
      }
   },
   "napi": {
      "binaryName": "swc-css-core",
      "packageName": "@swc/css-core",
      "targets": [
         "x86_64-apple-darwin",
         "x86_64-unknown-linux-gnu",
         "x86_64-pc-windows-msvc",
         "x86_64-unknown-linux-musl",
         "aarch64-unknown-linux-gnu",
         "aarch64-apple-darwin",
         "aarch64-pc-windows-msvc"
      ]
   },
   "devDependencies": {
      "@swc/css-types": "workspace:*"
   }
}